{
  "name": "runlogjs",
  "version": "0.2.0",
  "private": true,
  "license": "see LICENSE file",
  "description": "An activity log, mostly for exercise, mostly for running, mostly for fern",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferng/runlogjs"
  },
  "keywords": [
    "fern",
    "exercise",
    "running",
    "log"
  ],
  "author": "fern gonzalez",
  "bugs": {
    "url": "https://github.com/ferng/runlogjs/issues"
  },
  "homepage": "https://thecrunchycorner.com/runlog",
  "main": "server.js",
  "scripts": {
    "start": "node server.js | bunyan",
    "start-plain": "node server.js",
    "test-front": "babel-tape-runner -r test/helpers/prepTestJsdom.js test/public/**/*js",
    "test-front-one": "babel-tape-runner -r test/helpers/prepTestJsdom.js test/public/LapListSpec.js",
    "test-back": "babel-tape-runner test/src/**/*js",
    "test-back-one": "babel-tape-runner test/src/utils/dbConnSpec.js",
    "test": "npm run test-front | faucet & npm run test-back | faucet ",
    "webpack": "webpack",
    "web-dev": "webpack-dev-server",
    "docs": "npm run clean & jsdoc -c jsdoc.json",
    "docs-internal": "npm run clean & jsdoc -p -c jsdoc.json",
    "clean": "rm -r docs/*",
    "lint": "eslint.js ."
  },
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "dependency-tree": "^6.1.0",
    "docdash": "^0.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "faucet": "^0.0.1",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.10.0",
    "sinon": "^4.5.0",
    "tape": "^4.9.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.3"
  },
  "engines": {
    "node": "10.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
