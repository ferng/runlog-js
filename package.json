{
  "name": "runlogjs",
  "version": "0.1.0",
  "private": true,
  "license": "see LICENSE file",
  "description": "An activity log, mostly for exercise, mostly for running, mostly for fern",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferng/runlogjs"
  },
  "keywords": [
    "fern",
    "exercise",
    "running",
    "log"
  ],
  "author": "fern gonzalez",
  "bugs": {
    "url": "https://github.com/ferng/runlogjs/issues"
  },
  "homepage": "https://thecrunchycorner.com/runlog",
  "main": "server.js",
  "scripts": {
    "start": "node server.js | bunyan",
    "start-plain": "node server.js",
    "test-front": "babel-tape-runner -r test/helpers/prepTestJsdom.js test/public/**/*js",
    "test-front-one": "babel-tape-runner -r test/helpers/prepTestJsdom.js test/public/LapEntrySpec.js",
    "test-back": "babel-tape-runner test/src/**/*js",
    "test-back-one": "babel-tape-runner test/src/utils/dbConnSpec.js",
    "test": "npm run test-front | faucet & npm run test-back | faucet ",
    "webpack": "webpack",
    "web-dev": "webpack-dev-server",
    "docs": "npm run clean & jsdoc -c jsdoc.json",
    "docs-internal": "npm run clean & jsdoc -p -c jsdoc.json",
    "clean": "rm -r docs/*",
    "lint": "eslint.js ."
  },
  "dependencies": {
    "async": "2.1.4",
    "body-parser": "1.4.3",
    "bunyan": "1.8.4",
    "express": "4.4.5",
    "mongodb": "2.2.22",
    "prop-types": "^15.5.6",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.23.0",
    "babel-tape-runner": "2.0.1",
    "dependency-tree": "^6.0.0",
    "docdash": "^0.4.0",
    "enzyme": "2.7.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "faucet": "0.0.1",
    "jsdoc": "3.4.3",
    "jsdom": "9.12.0",
    "react-addons-test-utils": "15.4.2",
    "sinon": "2.1.0",
    "tape": "4.6.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "engines": {
    "node": "8.9.4"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
