{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./fernJs",
            "./public",
            "./shared",
            "./src",
            "./test",
            "./README.md"
        ],
        "exclude": [ /* array of paths to exclude */],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "docdash": {
        "static": true,
        "sort": true
    },
    "plugins": [
        "plugins/markdown"
    ]
}